{
    "name": "rentals-v2-backend",
    "version": "1.0.0",
    "description": "Rentals V2 Backend",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "start": "CONFIG=config.yaml nest start",
        "dev": "CONFIG=config.yaml nest start --watch",
        "start:dev": "CONFIG=config.yaml nest start --watch",
        "start:debug": "CONFIG=config.yaml nest start --debug --watch",
        "start:prod": "CONFIG=config.yaml node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,json}\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^11.1.1",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.1",
        "@nestjs/event-emitter": "^3.0.1",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/mongoose": "^11.0.3",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.1",
        "@nestjs/swagger": "^11.2.0",
        "access-log": "^0.4.1",
        "bcrypt": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "compression": "^1.8.0",
        "cookie-parser": "^1.4.7",
        "ejs": "^3.1.10",
        "helmet": "^8.1.0",
        "js-yaml": "^4.1.0",
        "mongodb": "~6.16.0",
        "mongoose": "^8.14.3",
        "nestjs-cls": "^5.4.3",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "uuid": "^11.1.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.7",
        "@nestjs/testing": "^11.1.1",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.8",
        "@types/express": "^5.0.1",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "22.15.18",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.0",
        "jest": "29.7.0",
        "prettier": "^3.5.3",
        "ts-jest": "29.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
